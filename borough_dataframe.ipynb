    "Bronx = []\n",
    "Brook = []\n",
    "Stat = []\n",
    "Queens = []\n",
    "Man = []\n",
    "\n",
    "\n",
    "count = 0\n",
    "for item in NYC_zips[\"ZIP Code\"]:\n",
    "    bur = NYC_zips.iloc[count,1]\n",
    "    if bur == \"Bronx\":\n",
    "        Bronx.append(item)\n",
    "    if bur == \"Brooklyn\":\n",
    "        Brook.append(item)\n",
    "    if bur == \"Queens\":\n",
    "        Queens.append(item)\n",
    "    if bur == \"Manhattan\":\n",
    "        Man.append(item)\n",
    "    if bur == \"Staten Island\":\n",
    "        Stat.append(item)\n",
    "    count +=1\n",
    "    \n",
    "\n",
    "#lotto_by_burough\n",
    "\n",
    "\n",
    "\n",
    "mydict = [{'Bronx': len(Bronx), 'Brooklyn': len(Brook), 'Queens': len(Queens),'Manhattan': len(Man),'Staten Island': len(Stat),}]\n",
    "\n",
    "mydict\n",
    "\n",
    "lotto_by_burough=pd.DataFrame(mydict)\n",
    "\n",
    "lotto_by_burough\n"
